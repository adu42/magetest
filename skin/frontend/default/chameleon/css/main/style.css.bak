/***************
	Header
***************/
.main-header {
    display:block;
    position:relative;
    padding:0;
	overflow:hidden;
	}
.header {
    clear:both;
	overflow:hidden;
	} 
.logo {	
	display:block;
	float:left;
	width:235px;
	padding:0 6px;
	position:absolute;
	left:0;top:0;
	}
.pink .logo #pink,
.green .logo #green,
.blue .logo #blue,
.darkblue .logo #darkblue,
.brown .logo #brown,
.red .logo #green{
	display:block!important;
	}
/***************
	Navigation
***************/
.user-menu-button,
.filters-block .actions a{
	float:right;
	width:70px;
	height:24px;
	text-align:center;
	display:block;
	font:10px/24px Arial;
	border-radius:35px;
	box-shadow:1px 2px 4px rgba(0, 0, 0, .20);
	}
.black .user-menu-button,
.black .filters-block .actions a,
.white .user-menu li a:hover,
.white .user-menu li.active a ,
.black .user-menu li a,
.red .user-menu li a
.darkblue .user-menu li a,
.black li.cart-icon > a:hover span strong,
.black li.cart-icon.active > a span strong {
	color:#ffffff;
	}
.red  .pages ul li.current span,
.red  .pages li a:hover,
.green  .pages ul li.current span,
.green  .pages li a:hover{
	border:1px solid #80732B;
	}
/* User Navigation
-----------------------------*/
.header-top {
	border-top:0;
	border-right:0;
	border-left:0;
	height:40px;
	border-top:1px solid #cccbcb;
	border-bottom:1px solid #ccc;
	background:#ddd;
	background-image:-webkit-gradient(linear,left top,left bottom,from(#efefef),to(#d7d7d7));
	}
.user-menu{position:absolute;right:0;top:0;}
.user-menu li {
	float:left;
	display:inline;
	margin-right:5px;
	}
.user-menu li a,
.user-menu li.active a {
	text-align:center;
	height:38px;
	line-height:38px;
	font:10px Arial;
	color:#ffffff;
	}
.user-menu li.active a,
.user-menu li a:hover{
	border:none!important;
	}
.white .user-menu li a{
	color:#222222;
	}
.darkblue .user-menu li a:hover,
.darkblue .user-menu li.active a{
	color:#072144;
	}

.green .user-menu li.active a,
.green .user-menu li a:hover,
.green  li.cart-icon > a span strong,
.green .pages ul li.current span,
.green .pages li a:hover{
	background: #28641f; /* Old browsers */
