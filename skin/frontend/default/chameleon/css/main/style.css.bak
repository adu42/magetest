 /********** Mage_CSS Common Elements ***********/
html { 
	-webkit-text-size-adjust:none;
	-ms-text-size-adjust:none;
	overflow-y:scroll;
	}
html, body {
	height:100%;
	width:100%;
	}
BODY  img{
	max-width: 100%;
	}
body {
	font:normal 12px HelveticaNeue, Arial, Verdana, sans-serif;
	}
body  table td .product-name,
body  table td .product-name a{word-wrap:break-word;}
body.red, 
body.green{
	background:url( '../../images/bg-inner.png' ) repeat!important;
	}
body.darkblue{
	background:#151a38!important;
	}
body.brown {
	background:#070707;
	}
body.white{color:#222222;}
body.black{color:#000000;}

.belvg {
	box-shadow:0px 2px 3px rgba(0, 0, 0, .20);
	}
.global-site-notice { 
	-webkit-transform:translate3d(0, 0, 1px); 
	}
/*BACKGROUND*/
.white .belvg,
.black .belvg,
.white .header,
.black .header,
.darkblue .belvg{
	background:#ffffff;
	}
.red .belvg,
.red .header,
.green .belvg,
.green .header{
	background:#f5f2e0;
	}
.blue .belvg,
.blue .header,
.pink .belvg,
.pink .header{
	background:#fdfdfd;
	}
.darkblue .header{
	background:#1c3a61;
	}
.brown .header{
	background:#332824;
	}
.brown .belvg{
	background:url( '../../images/bg-brown.jpg' ) repeat left top!important; 
	}
body > .belvg img {
    max-width: 100%;
	}	
