/* Categories Navigation
-----------------------------*/
 .nav-wrap {
	clear:both;
    overflow:hidden;
	}
#nav-container {
    overflow:hidden;
    padding:0;
	margin-top:0;
    position:relative;
    -webkit-transform:translate3d(0, 0, 0);
    -webkit-transition:-webkit-transform 250ms linear;
        -o-transition:-o-transform 250ms linear;
           transition:transform 250ms linear;	
	}
.brown #nav-container,
.darkblue #nav-container {
	margin-top:20px;
	}
#nav-container:after { 
	content: "."; 
	display: block; 
	clear: both; 
	visibility: hidden; 
	line-height: 0;
	height: 0; 
	}
#nav-container ul { 
	font-size:10px;
	margin:0; 
	width:100%;
	float:left; 
	border-bottom:1px solid #ccc;
	}
#nav-container li a {
    display:block;
	}
#nav-container li.subcategory-header{
	margin-right: 36px;
	height:28px;
	padding:6px 10px;
	-moz-border-radius:15px 15px 0 0!important;
	-webkit-border-radius:15px 15px  0 0!important;
	border-radius:15px 15px 0 0!important;
	text-align:center;
	border-bottom:0!important;
	}	
#nav-container li,
#nav-container li.level-top,
#nav-container li.level1 {
	padding:0 36px 0 0;
	}
#nav-container li.level-top > a {
	border-radius:15px;
	}
#nav-container li.subcategory-header span {
	font:bold 10px/20px Arial;
	}
.white #nav-container li.subcategory-header span {
	color:#030303;
	text-shadow: 0px 1px 1px #cbcbcb;
	}
.black #nav-container li.subcategory-header span {
	color:#fdfdfd;
	text-shadow: 0px 1px 1px #000000;
	}
#nav-container li.subcategory-header .button-wrap {
	float:left;
	position:absolute;
	}
#nav-container li.subcategory-header .button-wrap button {
	border:none!important;
	box-shadow:none!important;
	padding-left:10px;
    display:inline-block;
    font:12px/25px Arial;
	text-align:center;
    margin:0 0 0 10px;
    position:relative;
    z-index:3;
    -webkit-appearance:none;
            appearance:none;
	}
.white #nav-container li.subcategory-header .button-wrap button{
    background:url(../../images/icons/arrow-prevw.png) no-repeat left center;
